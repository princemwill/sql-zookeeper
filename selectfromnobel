--  3 SELECT from Nobel - challenge 4
-- #1
SELECT yr, subject, winner
  FROM nobel
 WHERE yr = 1950;
-- #2
SELECT winner
  FROM nobel
 WHERE yr = 1962
   AND subject = 'Literature';
-- #3
SELECT yr, subject 
FROM nobel
WHERE winner = 'Albert Einstein';
-- #4
select winner
FROM nobel 
WHERE subject = 'peace'
AND yr >= 2000;
-- #5
select yr, subject, winner
from nobel
where subject = 'literature'
and yr >= 1980 and yr <=1989;
-- #6
SELECT * FROM nobel
 WHERE winner IN ('Theodore Roosevelt',
                  'Woodrow Wilson',
                  'Jimmy Carter');
-- #7
select winner from nobel
where winner like 'John%';
-- #8
